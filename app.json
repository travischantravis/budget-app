{
  "expo": {
    "name": "first",
    "slug": "first",
    "version": "1.0.0",
    "description": "A mobile app that keeps track of my spending, and provides insights about my consumption pattern, such as how frequent I buy the same item.",
    "privacy": "public",
    "orientation": "portrait",
    "githubUrl": "https://github.com/travischantravis/budget-app",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
